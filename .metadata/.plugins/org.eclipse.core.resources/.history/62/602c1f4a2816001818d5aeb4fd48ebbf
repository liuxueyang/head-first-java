 */


public class SimpleDotComTestDrive {
	public static void main(String[] args) {
		int[] x = {3, 4, 5};
		A a = new A();
		
		a.seta(x);
		a.printA();
		
		for (int y : x) {
			int[] z = {7, 8, y};
			a.seta(z);
			a.printA();
		}
		
		a.printA();
	}
}

class A {
	int[] a;
	
	void printA() {
		System.out.print("array a is: ");
		
		for (int x : a) {
			System.out.print(x + " ");
		}
		System.out.println();
	}
	
	void seta(int[] x) {
		a = x;
	}
}
